{
  "name": "vegis-website-react",
  "version": "1.0.0",
  "description": "The official website for Vegistechnology",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install  --prefix client",
    "start": "node server.js",
    "client": "npm run start --prefix client",
    "server": "nodemon server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "keywords": [
    "WebDevelopment",
    "Nodejs",
    "ReactJS",
    "MongoDB",
    "express",
    "JavaScript"
  ],
  "engines": {
    "node": "10.15.3",
    "npm": "6.9.1-next.0",
    "yarn": "1.15.2"
  },
  "author": "Uzoanya Dominic",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^4.1.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.5",
    "express-force-https": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "twilio": "^3.33.3",
    "validator": "^11.0.0"
  }
}
